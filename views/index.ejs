<!--内容-->
<div id="content">
    <div class="content-main">
        <ul class="content-main-left">
            <% data.forEach((item, index)=>{ %>
            <li class="<%= index%2?"right":"" %>">
                <div class="pic"
                     style="background:url(/images/article/<%= index%2 + 1 %>.jpg) no-repeat center top/cover;"></div>
                <div class="title" style="background:url(/images/article/<%= index%2 + 1 %>.jpg) center top/100%;">
                    <div class="t-bg"></div>
                    <p class="t t-1"><a href="/article/<%=item._id%>"><%=item.title%></a></p>
                    <p class="t t-2">发布时间：<span><%=item.date.toLocaleDateString()%></span></p>
                    <p class="t t-3">浏览量：<span><%=item.pageviews%></span></p>
                </div>
                <div class="article">
                    <p class="article-content"><%-item.content%></p>
                    <div class="more"><a href="/article/<%=item._id%>">阅读全部</a></div>
                    <div class="comment">
                        <a href="/article/<%=item._id%>"><span><%=item.comment.length%></span>条评论</a>
                    </div>
                </div>
            </li>
            <% }) %>
